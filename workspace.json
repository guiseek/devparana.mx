{
  "version": 2,
  "projects": {
    "creator-desktop": {
      "projectType": "application",
      "root": "apps/creator/desktop",
      "sourceRoot": "apps/creator/desktop/src",
      "prefix": "devpr",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/creator/desktop",
            "index": "apps/creator/desktop/src/index.html",
            "main": "apps/creator/desktop/src/main.ts",
            "polyfills": "apps/creator/desktop/src/polyfills.ts",
            "tsConfig": "apps/creator/desktop/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/creator/desktop/src/favicon.ico",
              "apps/creator/desktop/src/assets"
            ],
            "styles": ["apps/creator/desktop/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/creator/desktop/src/environments/environment.ts",
                  "with": "apps/creator/desktop/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "creator-desktop:build:production"
            },
            "development": {
              "browserTarget": "creator-desktop:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "creator-desktop:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/creator/desktop/src/**/*.ts",
              "apps/creator/desktop/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/creator/desktop"],
          "options": {
            "jestConfig": "apps/creator/desktop/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "creator-desktop-e2e": {
      "root": "apps/creator/desktop-e2e",
      "sourceRoot": "apps/creator/desktop-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/creator/desktop-e2e/cypress.json",
            "tsConfig": "apps/creator/desktop-e2e/tsconfig.e2e.json",
            "devServerTarget": "creator-desktop:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "creator-desktop:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/creator/desktop-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "creator-feature-recorder": {
      "projectType": "library",
      "root": "libs/creator/feature-recorder",
      "sourceRoot": "libs/creator/feature-recorder/src",
      "prefix": "devpr",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/creator/feature-recorder"],
          "options": {
            "jestConfig": "libs/creator/feature-recorder/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/creator/feature-recorder/src/**/*.ts",
              "libs/creator/feature-recorder/src/**/*.html"
            ]
          }
        }
      }
    },
    "creator-feature-shell": {
      "projectType": "library",
      "root": "libs/creator/feature-shell",
      "sourceRoot": "libs/creator/feature-shell/src",
      "prefix": "devpr",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/creator/feature-shell"],
          "options": {
            "jestConfig": "libs/creator/feature-shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/creator/feature-shell/src/**/*.ts",
              "libs/creator/feature-shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "creator-ui-shared": {
      "projectType": "library",
      "root": "libs/creator/ui-shared",
      "sourceRoot": "libs/creator/ui-shared/src",
      "prefix": "devpr",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/creator/ui-shared"],
          "options": {
            "project": "libs/creator/ui-shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/creator/ui-shared/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/creator/ui-shared/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/creator/ui-shared"],
          "options": {
            "jestConfig": "libs/creator/ui-shared/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/creator/ui-shared/src/**/*.ts",
              "libs/creator/ui-shared/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "creator-desktop"
}
