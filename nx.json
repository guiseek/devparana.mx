{
  "npmScope": "devparana",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "creator": {
      "tags": ["type:app"]
    },
    "creator-feature-recorder": {
      "tags": ["type:feature"]
    },
    "creator-feature-shell": {
      "tags": ["type:shell"]
    },
    "creator-ui-shared": {
      "tags": ["type:ui"]
    },
    "creator-util-recorder": {
      "tags": ["type:util", "scope:creator"]
    }
  }
}
